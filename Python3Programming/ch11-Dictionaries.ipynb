  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "159d4913",
   "metadata": {},
   "outputs": [],
   "source": [
    "inventory = {'apples': 430, 'bananas': 312, 'oranges': 525, 'pears': 217}\n",
    "# change the value of 'bananas'\n",
    "inventory['bananas'] = inventory['bananas'] + 200\n",
    "# returns the number of key-value pairs\n",
